local kavoUi = loadstring(game:HttpGet("https://pastebin.com/raw/vff1bQ9F"))()
local Window = kavoUi.CreateLib("DeadlyHub Jenga","Ocean")

--Tabs and Sections

local MainTab = Window:NewTab("Main")
local MainSection = 
MainTab:NewSection("MainSection")

local DeadlyTab = Window:NewTab("DeadlyHub")
local DeadlySection = 
DeadlyTab:NewSection("DeadlyHub")
 
--Main Buttons

MainSection:NewButton("End Game", "Only use on towers team", function()
   game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-65.85745239257812, 99.9999771118164, -18.305282592773438)
end)

MainSection:NewButton("NoKillBrick", "Removes the maps killbricks.", function()
game.Workspace.Map.Classic.KillBrick:Destroy()
end)

MainSection:NewButton("Remove Objects", "Removes the objects thrown by the Destroyer.", function()
game.Workspace.Projectiles:Destroy()
end)

MainSection:NewButton("Reset", "Resets your character.", function()
game.Players.LocalPlayer.Character["Head"]:Destroy()
end)

--Deadly Buttons

DeadlySection:NewButton("DeadlyHub", nil, function()
	loadstring(game:HttpGet('https://raw.githubusercontent.com/DeadlyFrenzy/DeadlyHubScriptRblx/main/DeadlyHub'))()
end)
